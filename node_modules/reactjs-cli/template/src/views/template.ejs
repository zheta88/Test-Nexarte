<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>Welcome to your new React app.</title>

  <link href="//cdnjs.cloudflare.com/ajax/libs/normalize/3.0.2/normalize.min.css" rel="stylesheet">
  <link href="//fonts.googleapis.com/css?family=Source+Sans+Pro:200,400,600|Source+Code+Pro:400" rel="stylesheet">
  <style>
    /* Disable overscroll (bouncing scroll effect on OS X) */
    html, body {
      width: 100%;
      height: 100%;
      overflow: hidden;
      background: #ececec;
      margin: 0;
    }
    /* This class keeps the content scrollable and allows to programmatically change
     * the scroll position (which becomes broken with the css for disabling overscroll) */
    #container, .scrollable {
      width: 100%;
      height: 100%;
      min-height: 100%;
      overflow: auto;
      -webkit-overflow-scrolling: touch;
    }
  </style>
</head>
<body>
  <div id="container"><%- output %></div>

  <script>/*
    here you can set any initial state globals like window.initStateVar = <%# JSON.stringify(initStateVar) %>;
    Assuming you calling the render('template' on server.js with the correct initStateVar.
  */</script>
  <%- cachify_js("/app.min.js") %>
</body>
</html>
